        <name>Hide fields until record submission</name>
        <order/>
        <script><![CDATA[function onLoad() {

    // Hide fields if record is not submitted to database
    if (g_form.isNewRecord()) {
        g_form.setVisible('number', false);
        g_form.setVisible('sys_created_on', false);
    } else {
        g_form.setVisible('number', true);
        g_form.setVisible('sys_created_on', true);
    }
}]]></script>
